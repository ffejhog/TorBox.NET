<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>TorboxNET</AssemblyName>
        <RootNamespace>TorboxNET</RootNamespace>
        <Title>Torbox.NET</Title>
        <Authors>ffejhog</Authors>
        <Description>A C3 Wrapper for the torbox.app api</Description>
        <RepositoryUrl>https://github.com/ffejhog/TorBox.NET</RepositoryUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageProjectUrl>https://github.com/ffejhog/TorBox.NET</PackageProjectUrl>
        <RepositoryType>GitHub</RepositoryType>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    </ItemGroup>

</Project>
